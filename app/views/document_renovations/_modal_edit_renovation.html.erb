<div class="modal-dialog modal-lg modal-dialog-centered">
  <div class="modal-content">
    <div class="modal-header g-brd-blue g-bg-blue">
      <h5 class="modal-title text-white"><strong><%= @title_modal%></strong></h5>
      <button type="button" class="close text-white" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
    </div>
    <div class="modal-body">
      <%= render partial: 'form', locals: { 
                                    form_id: 'form-document-renovation-edit',
                                    method: 'PATCH',
                                    document_renovation: @document_renovation, 
                                    assignments_document: @document_renovation.document.id,
                                    document_expire: @document_renovation.document.expires?,
                                    enable_expiration_date: (  @document_renovation.document.expires? &&  @document_renovation.document.allow_modify_expiration? )
                                  } %>
    </div>
  </div>
</div>
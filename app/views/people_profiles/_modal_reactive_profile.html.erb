<div class="modal-dialog" role="document">
  <div class="modal-content">
    <div class="modal-header">
      <h5 class="modal-title"> Reactivar perfil </h5>
      <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
    </div>
    <div class="modal-body">
      <%= form_with(url: reactive_profile_assignment_profile_path, id: 'form-person-profile') do |form| %>
        <%= form.hidden_field :assignment_profile_id, value: assignment_profile_id %>
        <div class="form-group g-mb-10">
          <%= form.label :start_date, 'Fecha inicio vigencia:', class: "col-form-label" %>
          <%= form.date_field :start_date, class: "form-control u-form-control"%>
          <small class="form-control-feedback text-danger start_date"></small>
        </div>
        <%= form.submit 'Reactivar', class: 'btn u-btn-teal g-mr-10' %>
        <button type="button" data-dismiss="modal" class="btn u-btn-red g-mr-10"> Cerrar </button>
      <% end %>
    </div>
  </div>
</div>